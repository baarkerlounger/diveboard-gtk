<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkAboutDialog" id="about">
    <property name="can-focus">0</property>
    <property name="modal">1</property>
    <property name="destroy-with-parent">1</property>
    <property name="program-name">Diveboard</property>
    <property name="version">0.01</property>
    <property name="copyright" translatable="yes">Copyright 2021 diveboard</property>
    <property name="comments" translatable="yes">A diving logbook for GNOME.</property>
    <property name="website">https://github.com/diveboard</property>
    <property name="website-label" translatable="yes">Github page</property>
    <property name="authors">baarkerlounger</property>
    <property name="translator-credits" translatable="yes" comments="Replace me with your names.">translator-credits</property>
    <property name="license-type">mit-x11</property>
  </object>
</interface>

